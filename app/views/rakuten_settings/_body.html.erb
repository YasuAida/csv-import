    <tbody class="stocks">
      <% @rakuten_settings.each do |rakuten_setting| %>
        <!--Ajax用に固有のidを設定-->
        <tr id="rakuten_setting-<%= rakuten_setting.id %>">
          <%= form_for(rakuten_setting, url: rakuten_setting_path(rakuten_setting.id), remote: true) do |f|  %>
            <td class="update" id="check-<%= rakuten_setting.id %>"><%= f.check_box :destroy_check, {}, "true", "false" %></td>       
            <td class="large"><%= rakuten_setting.start_date %>から</td>            
            <% if rakuten_setting.end_date.blank? %>
              <td class="large"><%= Date.today %>まで</td>
            <% else %>
              <td class="large"><%= rakuten_setting.end_date %>まで</td>
            <% end %>
            <td class="large"><%= rakuten_setting.end_sales %>まで</td>
            <td class="amount"><%= rakuten_setting.pc_rate %>％</td>
            <td class="amount"><%= rakuten_setting.mobile_rate %>％</td>                    
            <td class="update" id="submit-<%= rakuten_setting.id %>"><%= link_to "修正", edit_rakuten_setting_path(rakuten_setting), class: 'btn btn-default btn-xs'%></td>
            
            <!--チェックボックス更新用JQuery-->
            <script>
              $("#check-<%= rakuten_setting.id %>").change(function(e){
                $('#edit_rakuten_setting_<%= rakuten_setting.id %>').submit();
              });
            </script>
          <% end %>  
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= paginate @rakuten_settings %>
</div>