<div class="panel panel-default" id='rakuten-default'>
	<div class="panel-heading">
	  ダミー計上商品(新規登録画面)
	  <%= link_to "一覧画面へ", dummy_stocks_path, class: 'btn btn-default btn-sm'%>
	</div>
  <div class="panel-body">
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>      
          <th class="middle">仕入ID</th>          
          <th class="account">販売日</th>
          <th class="account">キャンセル日</th>         
          <th class="rate">数量</th>         
          <th class="update"></th>      
        </tr>
      </thead>
      <%= form_for(@dummy_stock) do |f| %>
        <tbody class="footer">
          <tr>       
            <td class="number"><%= f.text_field :stock_id, :size => "4"%></td> 
            <td class="account"><%= f.text_field :sale_date, :size => "7"%></td>
            <td class="account"><%= f.text_field :cancel_date, :size => "7" %></td>         
            <td class="rate"><%= f.text_field :number, :size => "2", :style=>"text-align:right"%></td>
            <td class="update"><%= f.submit "登録", class: 'btn btn-default btn-xs'%></td>
          </tr>  
        </tbody>
      <% end %>
    </table>
  </div>
</div>