<h1>TopPages#index</h1>
<p>
  BackUp:
  <%= link_to "ダウンロード", download_top_pages_path(format: "csv") %>
</p>

<%= form_tag({action: 'upload'}, {multipart: true}) do %>
  <%= file_field :upload, :datafile, {multiple: true}%>
  <%= submit_tag "アップロード" %>
<% end %>


<tr>
  <th>事業開始年月日</th>
</tr>
<tr>
  <%= form_for(@period) do |f|  %>        
    <td><%= f.text_field :period_start %></td>
    <td><%= f.submit "期首登録"%></td>
  <% end %>
</tr>
