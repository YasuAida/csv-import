<div class="panel panel-default">
	<div class="panel-heading">
	  マルチチャンネルＳＫＵ登録(新規登録画面)
	  <%= link_to "一覧画面へ", multi_channels_path, class: 'btn btn-default btn-xs'%>
	</div>
  <div class="panel-body">
    <table class="table table-striped table-bordered table-condensed">
      <thead>
        <tr>
          <th class="middle">取引ID</th>
          <th class="account">日付</th>            
          <th class="reference">注文番号</th>
          <th class="large">ＳＫＵ</th>
          <th class="rate">個数</th>
          <th class="update">更新</th> 
        </tr>
      </thead>
      <%= form_for(@multi_channel) do |f| %>
        <tbody class="footer">
          <tr>
            <td class="middle"><%= f.text_field :sale_id, :size => "4" %></td>
            <td class="account"><%= f.text_field :date, :size => "7"%></td>               
            <td class="reference"><%= f.text_field :order_num, :size => "20" %></td>           
            <td class="large"><%= f.text_field :sku, :size => "11"%></td>          
            <td class="rate"><%= f.text_field :number, :size => "2", :style=>"text-align:right"%></td>                
            <td class="update"><%= f.submit "登録", class: 'btn btn-default btn-xs' %></td>
          </tr>
        </tbody>
      <% end %>
    </table>
  </div>
</div>