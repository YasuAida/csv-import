<div class="panel panel-default">
	<div class="panel-heading">
	  経費台帳(一覧画面)
	  <%= link_to "新規登録画面へ", new_expenseledger_path, class: 'btn btn-default btn-sm'%>
	</div>
  <div class="panel-body"> 
    <%= form_tag({action: 'upload'}, {multipart: true, class: "form_tag"}) do %>
      <label>CSVデータの取込み:</label>
      <%= file_field :upload, :datafile, class: 'filestyle', "data-buttonText": "ファイル選択", 'data-input': false %>
      <%= submit_tag "UPLOAD", class: 'btn btn-default' %>
    <% end %>
  </div>
</div>